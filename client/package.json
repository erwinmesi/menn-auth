{
  "name": "menn",
  "version": "1.0.0",
  "description": "MongoDB Express Nuxt Node Project",
  "author": "Erwin Mesias",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && cp .htaccess ./../server/public",
    "start": "nuxt start",
    "generate": "nuxt generate && cp .htaccess ./../server/public",
    "deploy": "push-dir --dir=dist --branch=dist --cleanup --force",
    "gen-deploy": "npm run generate && npm run deploy",
    "build-deploy": "npm run build && npm run deploy"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "lodash.get": "^4.4.2",
    "nuxt": "^2.3.4",
    "sentence-case": "^2.1.1",
    "toastr": "^2.1.4",
    "vee-validate": "^2.1.7",
    "vuetify": "^1.3.14",
    "vuetify-loader": "^1.0.8"
  },
  "devDependencies": {
    "node-sass": "^4.11.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
